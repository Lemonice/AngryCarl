          <argument>
            <kind>1</kind>
            <string>// Controls
if (keyboard_check(vk_up)||keyboard_check(ord('W')))
{
    if (direction != 2){
        direction = 2;
    }
    y -= 5;
}
if (keyboard_check(vk_down)||keyboard_check(ord('S')))
{
    if (direction != 4){
        direction = 4;
    }
    y += 5;
}
if (keyboard_check(vk_left)||keyboard_check(ord('A')))
{
    if (direction != 1){
        if (sprite_index != spr_gdgyl &amp;&amp; sprite_index != spr_gdgyhrtl){
            if (damage_cooldown &lt; 35){
                sprite_index = spr_gdgyl;
            } else {
                sprite_index = spr_gdgyhrtl;
            }
        }
        direction = 1;
    }
    x -= 5;
}
if (keyboard_check(vk_right)||keyboard_check(ord('D')))
{
    if (direction != 3){
        if (sprite_index != spr_gdgy &amp;&amp; sprite_index != spr_gdgyhrt){
            if (damage_cooldown &lt; 35){
                sprite_index = spr_gdgy;
            } else {
                sprite_index = spr_gdgyhrt;
            }
        }
        direction = 3;
    }
    x += 5;
}

// Weapon selection
if (keyboard_check(ord('1'))){
    weapon = 1;
} else if (keyboard_check(ord('4'))){
    weapon = 4;
}


// Shooting check
if (keyboard_check(vk_space)){
    firing = true;
} else {
    firing = false;
}

// Weapon maintenance and cooldown
if (cooldown &gt; 0){
    cooldown--;
}

// Getting hurt cooldown
if (damage_cooldown &gt; 0){
    damage_cooldown--;
    if (damage_cooldown &lt; 35){
        if (sprite_index == spr_gdgyhrt){
            sprite_index = spr_gdgy;
        } else if (sprite_index == spr_gdgyhrtl){
            sprite_index = spr_gdgyl;
        }
    }
}

// Shooting logic
if (firing){
    origin_x_offset = 0;
    origin_y_offset = 0;
    
    switch(direction){
        case 1:
            origin_x_offset = -25;
            break;
        case 2:
            origin_y_offset = -25;
            break;
        case 3:
            origin_x_offset = 25;
            break;
        case 4:
            origin_y_offset = 25;
            break;
        default:
            break;
    }

    switch(weapon){
        case 1:
            if (cooldown == 0){
                projectile = instance_create(obj_gdgy.x + origin_x_offset, obj_gdgy.y + origin_y_offset, obj_bullet);
                projectile.direction = direction;
                
                // If the bullet is going to go up or down
                if (direction == 2 || direction == 4){
                    projectile.sprite_index = spr_bulletv;
                }
                
                cooldown = 10;
            }
            break;
        case 4:
            if (has_flames){
                projectile = instance_create(obj_gdgy.x + origin_x_offset, obj_gdgy.y + origin_y_offset, obj_flame);
                projectile.direction = direction;
            }
            break;
        default:
            break;
    }
}
</string>
