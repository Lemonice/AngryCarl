        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Controls
if (keyboard_check(vk_up)||keyboard_check(ord('W')))
{
    if (direction != 2){
        direction = 2;
    }
    y -= 5;
}
if (keyboard_check(vk_down)||keyboard_check(ord('S')))
{
    if (direction != 4){
        direction = 4;
    }
    y += 5;
}
if (keyboard_check(vk_left)||keyboard_check(ord('A')))
{
    if (direction != 1){
        if (sprite_index != sgdgyl &amp;&amp; sprite_index != sgdgyhrtl){
            if (damage_cooldown &lt; 35){
                sprite_index = sgdgyl;
            } else {
                sprite_index = sgdgyhrtl;
            }
        }
        direction = 1;
    }
    x -= 5;
}
if (keyboard_check(vk_right)||keyboard_check(ord('D')))
{
    if (direction != 3){
        if (sprite_index != sgdgy &amp;&amp; sprite_index != sgdgyhrt){
            if (damage_cooldown &lt; 35){
                sprite_index = sgdgy;
            } else {
                sprite_index = sgdgyhrt;
            }
        }
        direction = 3;
    }
    x += 5;
}

// Weapon selection
if (keyboard_check(ord('1'))){
    weapon = 1;
} else if (keyboard_check(ord('2'))){
    weapon = 2;
} else if (keyboard_check(ord('3'))){
    weapon = 3;
} else if (keyboard_check(ord('4'))){
    weapon = 4;
}


// Shooting check
if (keyboard_check(vk_space)){
    firing = true;
} else {
    firing = false;
}

// Weapon maintenance and cooldown
if (cooldown &gt; 0){
    cooldown--;
}
if (mg_cooldown &gt; 0){
    mg_cooldown--;
}
if (sg_cooldown &gt; 0){
    sg_cooldown--;
}

// Getting hurt cooldown
if (damage_cooldown &gt; 0){
    damage_cooldown--;
    if (damage_cooldown &lt; 35){
        if (sprite_index == sgdgyhrt){
            sprite_index = sgdgy;
        } else if (sprite_index == sgdgyhrtl){
            sprite_index = sgdgyl;
        }
    }
}

// Shooting logic
if (firing){
    origin_x_offset = 0;
    origin_y_offset = 0;
    
    switch(direction){
        case 1:
            origin_x_offset = -25;
            break;
        case 2:
            origin_y_offset = -25;
            break;
        case 3:
            origin_x_offset = 25;
            break;
        case 4:
            origin_y_offset = 25;
            break;
        default:
            break;
    }

    switch(weapon){
        case 1:
            if (cooldown == 0){
                projectile = instance_create(ogdgy.x + origin_x_offset, ogdgy.y + origin_y_offset, obullet);
                projectile.direction = direction;
                
                // If the bullet is going to go up or down
                if (direction == 2 || direction == 4){
                    projectile.sprite_index = sbulletv;
                }
                
                cooldown = 10;
            }
            break;
        case 2:
            if (has_machinegun){
                if (mg_cooldown == 0){
                    projectile = instance_create(ogdgy.x + origin_x_offset, ogdgy.y + origin_y_offset, obullet);
                    projectile.direction = direction;
                    
                    // If the bullet is going to go up or down
                    if (direction == 2 || direction == 4){
                        projectile.sprite_index = sbulletv;
                    }
                    
                    mg_cooldown = 5;
                }
            }
            break;
        case 3:
            if (has_shotgun){
                if (sg_cooldown == 0){
                    projectile = instance_create(ogdgy.x + origin_x_offset, ogdgy.y + origin_y_offset, oshotblast);
                    projectile.direction = direction;
                    
                    sg_cooldown = 25;
                }
            }
            break;
        case 4:
            if (has_flames){
                projectile = instance_create(ogdgy.x + origin_x_offset, ogdgy.y + origin_y_offset, oflame);
                projectile.direction = direction;
            }
            break;
        default:
            break;
    }
}
</string>
